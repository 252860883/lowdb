{
  "name": "lowdb",
  "version": "0.12.2",
  "description": "JSON database for Node and the browser powered by lodash",
  "keywords": [
    "flat",
    "file",
    "local",
    "database",
    "JSON",
    "lo-dash",
    "lodash",
    "underscore",
    "localStorage",
    "embed",
    "embeddable",
    "extendable"
  ],
  "main": ".",
  "scripts": {
    "test": "babel-node test/all | tap-spec",
    "prepublish": "npm run build",
    "precommit": "npm test",
    "build": "npm run commonjs && npm run umd && npm run min",
    "commonjs": "babel src --out-dir .",
    "umd": "npm run umd:lowdb && npm run umd:lowdb.localstorage",
    "min": "npm run min:lowdb && npm run min:lowdb.localstorage",
    "umd:lowdb": "webpack src/index.js dist/lowdb.js --output-library lowdb",
    "min:lowdb": "webpack src/index.js dist/lowdb.js --output-library lowdb -p",
    "umd:lowdb.localstorage": "webpack src/browser.js dist/lowdb.localstorage.js --output-library lowdb.localStorage",
    "min:lowdb.localstorage": "webpack src/browser.js dist/lowdb.localstorage.js --output-library lowdb.localStorage -p"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/typicode/lowdb.git"
  },
  "author": "Typicode <typicode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typicode/lowdb/issues"
  },
  "homepage": "https://github.com/typicode/lowdb",
  "dependencies": {
    "graceful-fs": "^3.0.8",
    "is-promise": "^2.1.0",
    "json-parse-helpfulerror": "^1.0.3",
    "steno": "^0.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-3": "^6.3.13",
    "husky": "^0.10.2",
    "lodash": "^4.3.0",
    "sinon": "^1.17.2",
    "standard": "^4.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "tempfile": "^1.1.1",
    "underscore-db": "^0.9.0",
    "webpack": "^1.12.13"
  },
  "peerDependencies": {
    "lodash": "^4"
  },
  "engines": {
    "node": ">= 0.12"
  },
  "browser": {
    "lodash": false
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
